{
  "name": "rnboilerplate",
  "version": "0.0.2-feature-init-project.2",
  "private": true,
  "scripts": {
    "android:debug": "react-native run-android --variant=debug --appIdSuffix staging",
    "android:staging": "react-native run-android --variant=stagingRelease --appIdSuffix staging",
    "android:production": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "dist:s-android": "sh ./script/s-dist-android.sh",
    "dist:p-android": "sh ./script/p-dist-android.sh",
    "dist:android": "fastlane android",
    "dist-codepush:s-android": "sh ./script/s-dist-push-android.sh",
    "bump-patch": "npm version patch --no-git-tag-version",
    "bump-minor": "npm version minor --no-git-tag-version",
    "bump-major": "npm version major --no-git-tag-version",
    "start": "react-native start",
    "test": "jest --coverage",
    "test-all": "jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007 -h 0.0.0.0",
    "build-storybook": "build-storybook",
    "bundle-visualizer": "npx react-native-bundle-visualizer"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-clipboard/clipboard": "^1.8.4",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-firebase/admob": "^11.5.0",
    "@react-native-firebase/analytics": "^11.5.0",
    "@react-native-firebase/app": "^11.5.0",
    "@react-native-firebase/auth": "^11.5.0",
    "@react-native-firebase/firestore": "^11.5.0",
    "@react-native-firebase/in-app-messaging": "^11.5.0",
    "@react-native-firebase/remote-config": "^11.5.0",
    "@react-native-google-signin/google-signin": "^6.0.1",
    "@react-navigation/devtools": "^6.0.1",
    "@react-navigation/native": "^6.0.0",
    "@react-navigation/stack": "^6.0.7",
    "@reduxjs/toolkit": "^1.6.1",
    "@sentry/react-native": "^3.1.0",
    "axios": "^0.22.0",
    "i18next": "^20.4.0",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^4.0.3",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-i18next": "^11.11.4",
    "react-native": "0.64.1",
    "react-native-code-push": "~6.2.1",
    "react-native-config": "^1.4.3",
    "react-native-device-info": "^8.1.7",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fast-image": "^8.3.7",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-modal": "^12.0.3",
    "react-native-modal-datetime-picker": "^10.2.0",
    "react-native-onesignal": "^4.2.0",
    "react-native-pager-view": "^5.4.0",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "^2.2.0",
    "react-native-render-html": "^6.0.5",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.4.0",
    "react-native-share": "^7.0.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-tracking-transparency": "^0.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.6.6",
    "react-navigation-collapsible": "^6.0.0",
    "react-query": "^3.27.0",
    "react-redux": "^7.2.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "sp-react-native-in-app-updates": "^1.1.3",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-ondevice-notes": "^5.3.23",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react-native": "^0.64.5",
    "@types/react-native-restart": "^0.0.14",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack-env": "^1.16.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "detox": "^18.23.1",
    "eslint": "^7.14.0",
    "eslint-plugin-detox": "^1.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.3.1",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-dom": "17.0.1",
    "react-native-flipper": "^0.108.0",
    "react-native-performance": "^2.0.0",
    "react-native-performance-flipper-reporter": "^2.0.0",
    "react-native-performance-monitor": "^1.2.1",
    "react-native-storybook-loader": "^2.0.4",
    "react-test-renderer": "17.0.2",
    "redux-saga-test-plan": "^4.0.3",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./jest/setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|@react-native-community|@react-navigation)"
    ]
  }
}
